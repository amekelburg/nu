- unless @subsection == :manage
  = icon_link_to 'person_add', :new_project_join
  = icon_link_to 'add', :new_project
  = icon_link_to 'storage', '#join-project-approve'
  - managed_projects = Rails.env.test? ? [] : deter_lab.get_managed_projects.reject(&:admin?)
  - if managed_projects.any?
    .settings
      = icon_link_to 'settings', "#", class: 'dropdown-toggle', data: { toggle: 'dropdown' }, role: 'button'
      %ul.dropdown-menu.dropdown-menu-right
        - managed_projects.each do |p|
          %li= link_to p[:project_id], manage_project_path(p[:project_id])
